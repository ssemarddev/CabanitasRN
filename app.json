{
  "expo": {
    "name": "cabanitas",
    "slug": "cabanitas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoCaba.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LogoCaba.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/LogoCaba.png",
      "build": {
      "output": "dist"
    }
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
